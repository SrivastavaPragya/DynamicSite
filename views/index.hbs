  {{!-- banner1 section --}}
  
  
  {{!-- service section --}}
  <div class="container">
    <section id="services_section" class="py-3">
    <h1 class="text-center text-secondary"> SERVICES WE PROVIDE</h1>
    <div class="row mt-5">
       {{!-- first service column --}}
{{#each services}}
     <div class="col-md-4">

      <div class="card">
        <div class="card-body">
      <div class="container text-center py-3">
        <i class="{{this.icon}} service_icon" ></i>
      </div>
          <h5 class="card-title">{{this.title}}</h5>
          <p>{{this.description}}</p>
          <a href="{{this.linkText}}">{{this.link}}</a>
          <a href="" class="ml-5">support</a>
        </div>
      </div>

     </div>
     


{{/each}}
     





     

    </div>

  </section>
    
  </div>

 
  {{!-- banner 2section --}}
  

<section class="banner_2section py-5">
