  {{!-- contact section --}}
  <section id="contact_us_section " class="py-5  background1">
<div class="container">
  <h3 class="text-center text-secondary">CONTATC US</h3>
  <div class="row">
    <div class="col-md-12">
      <form action="/process-contact-form"  method="post">
  <div class="form-group py-3">
    <label for="exampleInputEmail1"> Your Email address</label>
    <input name="email" type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email">
    <small id="emailHelp" class="form-text text-muted"></small>
  </div>


  <div class="form-group py-3">
    <label for="contactNumberField" class="form-label">Your contact Number</label>
    <input  name ="phone" type="number" class="form-control" id="exampleInputPassword1" placeholder="phn No">
  </div>

<div class="form-outline py-4">
  
  <label class="form-label" for="textAreaExample">Message</label>
  
  <textarea   name="query" class="form-control " id="textAreaExample1" rows="4" placeholder="enter your query"></textarea>

  
</div>

  <div class="container text-center py-5">
    
  <button type="submit" class="btn btn-dark">Submit</button>
  </div>


</form>
    </div>
  </div>
</div>

  </section>

